#https://www.hackerrank.com/challenges/text-wrap/problem
import textwrap

def wrap(string, max_width):
    stt = ''
    for i in range(0, len(string), max_width):
        new_string = string[i:i+max_width]    
        stt = stt + new_string + '\n'
    return stt

if __name__ == '__main__':